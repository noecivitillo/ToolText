<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@+id/gradient">
        <shape android:shape="oval">
            <solid android:color="@color/teal_100"/>
            <corners android:radius="2dp"/>
            <stroke android:color="@color/blue_grey_500"
                android:width="2dp"/>
        </shape>
    </item>
    <item
        android:drawable="@drawable/ic_done_blue_gray_24dp"
        android:bottom="2dp"
        android:left="2dp"
        android:right="2dp"
        android:top="2dp"/>
</layer-list>